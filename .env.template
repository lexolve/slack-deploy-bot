# Slack Configuration
# Get from: Slack App Settings > Basic Information > App Credentials > Signing Secret
SLACK_SIGNING_SECRET=

# User Authorization
# Get user IDs: Click user profile in Slack > ... > Copy member ID (format: U12345ABC)
ALLOWED_USERS=

# GCP Configuration
# PROJECT_ID: Where the Cloud Function is deployed
PROJECT_ID=

# STAGING_PROJECT_ID: Where staging builds are triggered
STAGING_PROJECT_ID=

# PROD_PROJECT_ID: Where production builds are triggered
PROD_PROJECT_ID=

# Function region (for logging/metadata)
FUNCTION_REGION=europe-north1
